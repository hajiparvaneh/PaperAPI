{
  "name": "@paperapi/sdk",
  "version": "0.0.0-dev",
  "description": "Official PaperAPI SDK for modern JavaScript runtimes; use your API key from https://paperapi.de (free sandbox 50 PDFs/mo, higher plans at /pricing).",
  "license": "MIT",
  "keywords": [
    "paperapi",
    "pdf",
    "wkhtmltopdf",
    "sdk"
  ],
  "homepage": "https://github.com/hajiparvaneh/PaperAPI/tree/main/sdk/js#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/hajiparvaneh/PaperAPI.git",
    "directory": "sdk/js"
  },
  "bugs": {
    "url": "https://github.com/hajiparvaneh/PaperAPI/issues"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --sourcemap --clean",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit --pretty",
    "lint": "npm run typecheck",
    "test": "npm run lint",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "rimraf": "^5.0.5",
    "tsup": "^7.2.0",
    "typescript": "^5.4.2"
  }
}
